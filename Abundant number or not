import java.util.*;
import java.io.*;
public class Main{
    public static void main(String[] args){
        Scanner obj=new Scanner(System.in);
        int a=obj.nextInt();
        int sum=0;
        for(int i=1;i<=a;i++){
            if(a%i==0){
            sum=sum+i;}}
            if(sum>a){
                System.out.println("It is an abundant number");
            }
            
            else{
                System.out.println("Not an abundant number");
            }
            
        
    }
}
